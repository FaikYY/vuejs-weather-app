    )\r\n          .then((res) => {\r\n            console.log(res)\r\n            return res.json();\r\n          })\r\n          .then(results => {\r\n            console.log(results)\r\n            this.state.weather = results;\r\n          });\r\n          \r\n    },\r\n     checkWeatherForBackground(){\r\n      if(this.state.weather.weather[0].main == 'Clear'){\r\n        console.log(\"Clear weather background\")\r\n        this.state.background_class = 'clear'\r\n      }\r\n      else if(this.state.weather.weather[0].main == 'Clouds' && Math.round(this.$store.state.weather.main.temp) > 16){\r\n        console.log(\"Clouds weather background\")\r\n        this.state.background_class = 'clouds-warm'\r\n      }\r\n      else if(this.state.weather.weather[0].main == 'Clouds' && Math.round(this.$store.state.weather.main.temp) < 16){\r\n        console.log(\"Clouds weather background\")\r\n        this.state.background_class = 'clouds-cold'\r\n      }\r\n      else if(this.state.weather.weather[0].main == 'Snow'){\r\n        console.log(\"Snow weather background\")\r\n        this.state.background_class = 'snow'\r\n      }\r\n      else{\r\n        console.log('other types of weather detected!!')\r\n      }\r\n    }\r\n  },\r\n});\r\n"]}]}