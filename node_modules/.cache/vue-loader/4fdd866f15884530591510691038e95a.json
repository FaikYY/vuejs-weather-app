ther.weather[0].main == \"Clouds\" &&\r\n        Math.round(this.$store.state.weather.main.temp) >= 16\r\n      ) {\r\n        console.log(\"Clouds weather background\");\r\n        return \"clouds-warm\";\r\n      } else if (\r\n        this.$store.state.weather.weather[0].main == \"Clouds\" &&\r\n        Math.round(this.$store.state.weather.main.temp) < 16\r\n      ) {\r\n        console.log(\"Clouds weather background\");\r\n        return \"clouds-cold\";\r\n      } else if (this.$store.state.weather.weather[0].main == \"Snow\") {\r\n        console.log(\"Snow weather background\");\r\n        return \"snow\";\r\n      } else {\r\n        return \"\";\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<!-- \r\n             /$$               /$$          \r\n            | $$              | $$          \r\n  /$$$$$$$ /$$$$$$   /$$   /$$| $$  /$$$$$$ \r\n /$$_____/|_  $$_/  | $$  | $$| $$ /$$__  $$\r\n|  $$$$$$   | $$    | $$  | $$| $$| $$$$$$$$\r\n \\____  $$  | $$ /$$| $$  | $$| $$| $$_____/\r\n /$$$$$$$/  |  $$$$/|  $$$$$$$| $$|  $$$$$$$\r\n|_______/    \\___/   \\____  $$|__/ \\_______/\r\n                     /$$  | $$              \r\n                    |  $$$$$$/              \r\n                     \\______/               \r\n-->\r\n<style>\r\n* {\r\n  margin: 0;\r\n  padding: 0;\r\n  box-sizing: border-box;\r\n}\r\n\r\nbody {\r\n  font-family: \"montserrat\", sans-serif;\r\n}\r\n\r\n#app {\r\n  background-image: url(\"./assets/default.jpg\");\r\n  background-size: cover;\r\n  background-position: bottom;\r\n  transition: 0.4s;\r\n}\r\n\r\n#app.clouds-warm {\r\n  background-image: url(\"./assets/clouds-warm.jpg\");\r\n}\r\n\r\n#app.clouds-cold {\r\n  background-image: url(\"./assets/clouds-cold.jpg\");\r\n}\r\n\r\n#app.snow {\r\n  background-image: url(\"./assets/snow.jpg\");\r\n}\r\n\r\n#app.clear {\r\n  background-image: url(\"./assets/clear.jpg\");\r\n}\r\nmain {\r\n  min-height: 100vh;\r\n  padding: 25px;\r\n\r\n  background-image: linear-gradient